<form id="date_form" name="date_form">
<table class="dtlTb" cellspacing="2" cellpadding="2" id="registerBaseInfo_div">
	<tr>
		<td colspan="4" class="heading">注册基本信息</td>
		<td>
		<input type="hidden" id="baseInfo_id" name="baseInfo_id" value="$!{baseInfo.id}"/>
		<input type="hidden" id="merchantStatus" name="merchantStatus" value="1"/>
		</td>
	</tr>
	<tr>
		<td width="150" class="lbl"><span class="red">*</span>商户名称:</td>
		<td width="300">
			<input type="text" id="merchantName" name="merchantName" value="$!{baseInfo.merchantName}" class="txt w180"/>
			&nbsp;<label class="small-orange">(营业执照注册名)</label>
		</td>
		<td width="150" class="lbl"><span class="red">*</span>店铺名称:</td>
		<td width="300"><input type="text" id="shopName" name="shopName" value="$!{baseInfo.shopName}" class="txt w180"/></td>
	</tr>
	<tr>
		<td width="150" class="lbl"><span class="red">*</span>发证机关:</td>
		<td width="300">
			<select id="certifyAuthOrg" name="certifyAuthOrg">
				<option #if("$!{authInfo.certifyAuthOrg}"=="0") selected  #end value="0">工商行政管理局</option>
				<option #if("$!{authInfo.certifyAuthOrg}"=="99") selected  #end value="99">其他</option>
			</select>
		</td>
		<td width="150" class="lbl"><span class="red">*</span>营业执照编码:</td>
		<td width="300"><input type="text" id="certifyCode" name="certifyCode" value="$!{authInfo.certifyCode}" class="txt w180"/></td>
	</tr>
	<tr>
		<td width="150" class="lbl"><span class="red">*</span>组织机构代码:</td>
		<td width="300" colspan="3"><input type="text" id="orgCode" name="orgCode" value="$!{authInfo.orgCode}" class="txt w180"/></td>
	</tr>
	<tr>
		<td width="150" class="lbl"><span class="red">*</span>营业执照有效时间:</td>
		<td width="300" colspan="2"><input type="text" id="certifyStartTime" name="certifyStartTime" value="$!{authInfo.certifyStartTime}" class="txt w180 date" onclick="xDatePicker(1);"/>
		~ <input type="text" id="certifyEndTime" name="certifyEndTime" value="$!{authInfo.certifyEndTime}" class="txt w180 date" onclick="xDatePicker(1);"/>
		</td>
		<td>有效期是否永久:
			<input type="radio" id="isPermanent" name="isPermanent" #if("$!{authInfo.certifyEndTime}"=="9999-12-31") checked #end value="2" onclick='checkIsPermanent(this)'/>
		</td>
	</tr>
	<tr>
		<td width="150" class="lbl"><span class="red">*</span>主营类型:</td>
		<td width="300">
		<div id="div_bussinessType"></div>
		<input type="hidden" id="bussinessType" name="bussinessType" value="$!{baseInfo.bussinessType}" class="txt w180"/>
		</td>
		<td width="150" class="lbl"><span class="red">*</span>费率:</td>
