							alert(data.errorMessage);
						}
					} else{
						alert("注册成功!");
						window.location.href="#relativeUrl('/easypay/enterpriseAccount/index.htm')";
					}
	             },
	             error : function() {
	             }
	      });
		}
	}
	//校验数据
	function verification(){
		var merchantName=$("#merchantName").val();
		if(validate(merchantName)){
			alert("商户名称为空！");
			return false;
		}
		var shopName=$("#shopName").val();
		if(validate(shopName)){
			alert("店铺名称为空！");
			return false;
		}
		var certifyAuthOrg=$("#certifyAuthOrg").val();
		if(validate(certifyAuthOrg)){
			alert("请选择营业执照发证机关！");
			return false;
		}
		var certifyCode=$("#certifyCode").val();
		if(validate(certifyCode)){
			alert("营业执照编码为空！");
			return false;
		}
		var phoneNo=$("#phoneNo").val();
		if(validate(phoneNo)){
			alert("法人/负责人手机号为空！");
			return false;
		}
		var linkMan=$("#linkMan").val();
