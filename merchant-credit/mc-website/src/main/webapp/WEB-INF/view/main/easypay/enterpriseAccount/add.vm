	function SaveOrUpdate(s){
		if(!verification()){
			return ;
		}
		var s=s;
		var query = $("#date_form").serialize();//序列化表格内容为字符串
		if(s==0){
			$("#saveOrbtn1").attr({"disabled":true});
			$("#saveOrbtn2").attr({"disabled":true});
			jQuery.ajax( {
	            url:"#relativeUrl('/easypay/enterpriseAccount/temporarySave.htm')",
