	function setTimeStartEnd(a, c, b, d) {
        var e = new Date,
        f = e.getFullYear(),
        g = e.getMonth(),
        e = e.getDate();
        c.value = formatDate(new Date(f, g, e + a), 'yyyy-MM-dd') ;
        d.value = formatDate(new Date(f, g, e + b), 'yyyy-MM-dd') ;
	}
	$(function(){
		$(".upload").each
        (function () {
        var button = $(this), interval;
        new AjaxUpload(button, {
            action: "#relativeUrl('/file/easypayUpload.htm')",
            name: 'uploadfield',
			responseType:'json',
            onSubmit: function (file, ext) {
				 if (!(ext && /^(jpg|jpeg|JPG|JPEG|PNG|png|gif|GIF|bmp|BMP|avi|AVI|MP4|mp4|WMA|wma|zip|ZIP|rar|RAR|wmv|WMV|rm|RM|rmvb|RMVB|3gp|3GP|mpeg|MPEG)$/.test(ext))) {
                    alert("文件格式不正确,请选择 正确的格式的文件!");
                    return false;
                }
                button.text('上传中');
                interval = window.setInterval(function () {
                    var text = button.text();
                    if (text.length < 10) {
                        button.text(text + '|');
                    } else {
                        button.text('上传中');
                    }
                }, 200);
            },
            onComplete: function (file, response) {
                window.clearInterval(interval);
                // enable upload button
                // this.enable();
				if(response.uploadSuccess ==true){
					alert('上传成功');
					button.hide();
					$("#file_" + button.attr("id")+"_oriFileName").text(response.oriFileName);
					button.text('立刻上传');
					$("#div_" + button.attr("id")).css("display","");
					$("#hidden_" + button.attr("id") +"_storeKey").val(response.storeKey);
					$("#img_" + button.attr("id") +"_storeKey").attr("src", "#relativeUrl('/file/easypayDownload.htm')?storeKey="+response.storeKey);
				}else{
					  if(response.uploadSuccess=='overMax'){
						showDiv('mask_box',response.errorMessage);
						return false;
					}

				}
            }
        });
    	});
    });
	
