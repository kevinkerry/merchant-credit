	            type:'post',
	            cache:false,
	            success:function(data) {
	            	$("#saveOrbtn1").removeAttr("disabled");
	            	$("#saveOrbtn2").removeAttr("disabled");
					if (!data.success){
						if(data.errorMessage==null){
							alert("保存失败！");
						}else{
							alert(data.errorMessage);
						}
					} else{
						alert("保存信息成功!");
						window.location.href="#relativeUrl('/easypay/enterpriseAccount/index.htm')";
					}
	             },
	             error : function() {
	             }
	      });
		}else if(s==1){
			$("#saveOrbtn1").attr({"disabled":true});
			$("#saveOrbtn2").attr({"disabled":true});
			jQuery.ajax( {
	            url:"#relativeUrl('/easypay/enterpriseAccount/saveSubmit.htm')",
				data:query,
	            type:'post',
	            cache:false,
	            success:function(data) {
	            	$("#saveOrbtn1").removeAttr("disabled");
	            	$("#saveOrbtn2").removeAttr("disabled");
					if (!data.success){
						if(data.errorMessage==null){
							alert("保存失败！");
